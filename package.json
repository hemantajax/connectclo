  "license": "MIT",
  "scripts": {
    "dev": "nx dev connectstore",
    "build": "nx build connectstore",
    "build:prod": "nx build connectstore --configuration=production",
    "postbuild": "npm run uncss",
    "uncss": "node tools/scripts/uncss.js",
    "start": "nx start connectstore",
    "preview": "nx preview connectstore",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "test": "nx run-many --target=test --all",
    "test:watch": "nx run-many --target=test --all --watch",
